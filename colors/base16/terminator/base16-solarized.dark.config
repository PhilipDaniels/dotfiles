[global_config]
  title_transmit_bg_color = "#dc322f"
  title_inactive_bg_color = "#002b36"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#073642:#dc322f:#859900:#b58900:#268bd2:#d33682:#2aa198:#eee8d5:#002b36:#dc322f:#859900:#b58900:#268bd2:#d33682:#2aa198:#fdf6e3"
    foreground_color = "#eee8d5"
    background_color = "#073642"
    cursor_color = "#eee8d5"

  [[Base16 Solarized Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#073642:#dc322f:#859900:#b58900:#268bd2:#d33682:#2aa198:#eee8d5:#002b36:#dc322f:#859900:#b58900:#268bd2:#d33682:#2aa198:#fdf6e3"
    foreground_color = "#eee8d5"
    background_color = "#073642"
    cursor_color = "#eee8d5"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Solarized Dark
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
